{
    "name": "uxf-templates-vue",
    "version": "0.1.1",
    "description": "Simple template for usage of UXF with Vue.js",
    "keywords": [
        "components",
        "ui",
        "uxf"
    ],
    "homepage": "https://github.com/hivesolutions/uxf_templates#readme",
    "bugs": {
        "url": "https://github.com/hivesolutions/uxf_templates/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/hivesolutions/uxf_templates.git"
    },
    "license": "Apache-2.0",
    "author": "Hive Solutions",
    "files": [
        "index.html",
        "css/**/*.css",
        "js/**/*.js",
        "res/**/*.ico",
        "res/**/*.png",
        "test/**/*.js",
        "vue/**/*.js",
        "vue/**/*.vue"
    ],
    "main": "js/main.js",
    "scripts": {
        "build": "webpack --mode production --config webpack.config.js",
        "build-dev": "webpack --mode development --config webpack.config.js",
        "dev": "webpack-dev-server --config webpack.config.js --mode development --watch --hot",
        "lint": "eslint \"./**/*.{js,vue}\"",
        "prettier": "prettier \"./**/*.{js,vue,json}\" --write",
        "test": "mocha --recursive",
        "upgrade": "npx sort-package-json && ncu -u",
        "webpack": "webpack --config webpack.config.js"
    },
    "dependencies": {
        "hive-layout": "^0.1.3",
        "uxf": "^0.2.2",
        "vue": "^2.6.10"
    },
    "devDependencies": {
        "uxf-webpack": "^0.2.3"
    }
}
